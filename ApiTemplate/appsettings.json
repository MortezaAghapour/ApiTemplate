{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=RaymonTaskDataBase;Integrated Security=True;trusted_connection=true;"
  },
  "AllowedHosts": "*",
  "JwtConfiguration": {
    "SecreteKey": "ASDFGHJKLZXCVBNM",
    "EncryptKey": "QWERTGFDSAZXCVBH",
    "Issuer": "Raymon",
    "Audience": "Raymon",
    "NotBeforeMinutes": 0,
    "ExpirationMinutes": 60
  },
  "IdentityConfiguration": {
    "PasswordRequireDigit": false,
    "PasswordRequireLowercase": false,
    "PasswordRequireNonAlphanumeric": false,
    "PasswordRequireUppercase": false,
    "LockoutAllowedForNewUsers": false,
    "UserRequireUniqueEmail": false,
    "PasswordRequiredUniqueChars": 1,
    "PasswordMinLength": 8,
    "LockoutMaxFailedAccessAttempts": 3,
    "UserAllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789",
    "LockoutDefaultLockoutTimeSpan": 20,
    "LockoutType": "Minute"
  }
}
